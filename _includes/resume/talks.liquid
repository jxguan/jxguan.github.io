<ul class="card-text font-weight-light list-group list-group-flush">
  {% for content in data[1] %}
    <li class="list-group-item">
      <div class="row">
        
        <div class="col-xs-10 cl-sm-10 col-md-10 mt-2 mt-md-0">
          <h6 class="title font-weight-bold ml-1 ml-md-4">
            <a href="{{ content.url }}">{{ content.title }}</a>
          </h6>
          <ul class="items">
            {% for item in content.venues %}
              <li>
                <span class="item">{{ item.name }} </span>
                <div class="col-xs-2 cl-sm-2 col-md-2 text-center" style="width: 75px; float: right">
                  <span class="badge font-weight-bold danger-color-dark text-uppercase align-middle" style="min-width: 75px"> {{ item.date }} </span>
        	      </div>
              </li>
            {% endfor %}
          </ul>
        </div>
      </div>
    </li>
  {% endfor %}
</ul>
